    local playerDropdown = Instance.new("Frame")
    playerDropdown.Name = "PlayerDropdown"
    playerDropdown.Size = UDim2.new(1, -10, 0, 30)
    playerDropdown.Position = UDim2.new(0, 0, 0, 80)
